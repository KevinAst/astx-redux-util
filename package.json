{
  "name": "astx-redux-util",
  "version": "0.1.0",
  "description": "Several redux reducer composition utilities.",
  "main": "lib/index.js",
  "scripts": {
    "COMMENT01": "***                                                                                   ",
    "COMMENT02": "*** Please refer to SCRIPTS.md for an overvie of the project's available npm scripts. ",
    "COMMENT03": "***                                                                                   ",
    "start": "npm run dev",
    "dev": "npm-run-all --parallel build:watch test:lib:watch",
    "build": "webpack --progress --colors",
    "build:watch": "npm run build -- --watch",
    "build:for:bundle": "cross-env BABEL_ENV=commonjs NODE_ENV=development          npm run build",
    "build:for:bundle.min": "cross-env BABEL_ENV=commonjs NODE_ENV=production           npm run build",
    "build:for:lib": "cross-env BABEL_ENV=commonjs      babel src --out-dir lib --ignore spec,tooling",
    "build:for:es":  "cross-env BABEL_ENV=es            babel src --out-dir es  --ignore spec,tooling",
    "build:for:all": "npm-run-all build:for:bundle build:for:bundle.min build:for:lib build:for:es",
    "build:clean": "rimraf dist lib es",
    "test": "mocha --compilers js:babel-core/register --colors \"src/**/*.spec.js\"",
    "test:lib": "npm run test:samples -- --invert",
    "test:lib:watch": "npm run test:lib -- --watch",
    "test:samples": "npm run test -- --grep \"verify sample\"",
    "test:samples:watch": "npm run test:samples -- --watch",
    "test:all": "npm run test --",
    "test:all:watch": "npm run test:all -- --watch",
    "test:on:src": "cross-env MODULE_PLATFORM=src        npm run test:all",
    "test:on:bundle": "cross-env MODULE_PLATFORM=bundle     npm run test:all",
    "test:on:bundle.min": "cross-env MODULE_PLATFORM=bundle.min npm run test:all",
    "test:on:lib": "cross-env MODULE_PLATFORM=lib npm run test:all",
    "test:on:es":  "cross-env MODULE_PLATFORM=es  npm run test:all",
    "test:on:all": "npm-run-all test:on:src test:on:bundle test:on:bundle.min test:on:lib  test:on:es",
    "docs": "./node_modules/.bin/jsdoc --configure ./src/docs/jsdoc.conf.json --verbose",
    "docs:clean": "rimraf docs",
    "lint": "eslint src",
    "clean": "npm run build:clean && npm run docs:clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KevinAst/astx-redux-util.git"
  },
  "keywords": [
    "astx",
    "flux",
    "redux",
    "reducer",
    "redux reducer",
    "action",
    "compose",
    "composition",
    "higher order",
    "functional decomposition",
    "switch",
    "case",
    "utility",
    "util",
    "utils",
    "helper",
    "helpers"
  ],
  "author": "Kevin J. Bridges <kevinast@gmail.com> (https://github.com/KevinAst)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KevinAst/astx-redux-util/issues"
  },
  "homepage": "https://github.com/KevinAst/astx-redux-util",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "cross-env": "^3.2.3",
    "docdash": "^0.4.0",
    "eslint": "^3.16.1",
    "eslint-loader": "^1.6.3",
    "expect": "^1.20.2",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.3.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.1",
    "redux": "^3.6.0",
    "rimraf": "^2.5.4",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "lodash.identity": "^3.0.0"
  }
}
